{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "airbnb", "prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "prefer-const": "warn",
    "no-param-reassign": "warn",
    "no-console": "off",
    "no-unused-vars": "warn",
    "no-useless-escape": "warn"
  }
}
